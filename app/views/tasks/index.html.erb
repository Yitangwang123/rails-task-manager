<h1>My tasks</h1>

<% @tasks.each do |task| %>
  <%= link_to "#{task.title}", task_path(task) %>
  <%= task.completed %>
  <%= link_to "Delete",
            task_path(task),
            method: :delete,
            data: { confirm: "Delete #{task.title} ?" } %>
<% end %>

<%= link_to "Add a new task", new_task_path %>


